##### Below command is used to extract the 
for i in $(cat /mnt/weka/sims/punith.m/all_about_nodes/b1_b2_node_IP_details | grep -vE 'sim-ana|storage' | grep -oE '10.253.[0-9]{1,3}.[0-9]{1,3}'); do shm_value=$(timeout 5s ssh -o 'StrictHostKeyChecking=no' $i 'bash -s' < /mnt/weka/sims/punith.m/puni_scripts/get_dev_shm_usage.sh 2>/dev/null); if [ $(echo "$shm_value" | awk '{print $3}' | grep -oE '[0-9]{1,3}') -gt 80 ] 2>/dev/null; then echo "$shm_value"; fi; done
